[user]
        name = Raniere Silva
        email = raniere@rgaiacs.com
[alias]
        b = branch -v
        bm = branch --merged
        brm = branch --delete
        c = commit
        ca = commit --all
        caa = commit --all --amend
        cam = commit --all --message
        ck = checkout
        cp = cherry-pick
        d = diff
        dc = diff --cached
        dcm = diff --cached --minimal
        dcn = diff --cached --name-only
        dcw = diff --cached --word-diff
        dm = diff --minimal
        dn = diff --name-only
        ds = diff -b
        dw = diff --word-diff
        fall = fetch --all
        lg = log --oneline --graph --decorate
        lga = log --all --decorate --graph --oneline
        lgaf = log --oneline --graph --decorate --all --flow
        lgb = log --oneline --graph --decorate --branches
        lgbf = log --oneline --graph --decorate --branches --flow
        lgf = log --oneline --graph --decorate --flow
        lgm = log --oneline --graph --decorate --merges
        lgmf = log --oneline --graph --decorate --merges --flow
        pf = push --force
        pp = push --prune
        ppn = push --prune --dry-run
        prm = push --force --delete
        ra = remote dd
        rc = clean -f
        rcall = clean -fxd
        rcrf = clean -fd
        rv = remote -v
        shb = show -s
        shm = show --minimal --ignore-space-at-eol -b -w --ignore-blank-lines
        shn = show --name-only
        shs = show --summary -M
        shw = show --word-diff
        srm = clean -n
        st = status -s -b
        sta = status -s -b --ignored
        a = add
        i = init
        p = push --set-upstream
[core]
        editor = vim
        excludesfile = /home/raniere/.gitignore_global
        whitespace = trailing-space,space-before-tab
        pager = less -+S
[color]
        ui = auto
[color "diff"]
        whitespace = red reverse
[push]
        default = upstream
[tag]
        sort = v:refname
